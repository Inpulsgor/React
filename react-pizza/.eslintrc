{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": ["prettier", "airbnb", "plugin:react/recommended", "plugin:node/recommended"],
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    },
    "ecmaVersion": 12,
    "sourceType": "module"
  },
  "plugins": ["react", "@typescript-eslint", "prettier"],
  "rules": {
    "prettier/prettier": "error",
    "no-unused-vars": "warn",
    "no-console": "off",
    // "func-names": "off",
    "no-process-exit": "off",
    // "object-shorthand": "off",
    "class-methods-use-this": "off"
  }
}
